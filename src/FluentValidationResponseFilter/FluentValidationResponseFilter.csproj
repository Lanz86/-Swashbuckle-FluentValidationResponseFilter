<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Nullable>enable</Nullable>
	  <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
	  <Title>Swashbuckle FluentValidationResponseFilter</Title>
	  <Authors>Antonio Lanzolla</Authors>
	  <PackageProjectUrl>https://github.com/Lanz86/Swashbuckle-FluentValidationResponseFilter</PackageProjectUrl>
	  <RepositoryUrl>https://github.com/Lanz86/Swashbuckle-FluentValidationResponseFilter</RepositoryUrl>
	  <Version>1.0.0.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="FluentValidation" Version="11.4.0" />
    <PackageReference Include="Swashbuckle.AspNetCore.SwaggerGen" Version="6.5.0" />
  </ItemGroup>

</Project>
